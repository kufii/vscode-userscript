{
  "name": "vscode-userscript",
  "displayName": "UserScript",
  "description": "UserScript language support for VS Code",
  "version": "1.3.2",
  "publisher": "adpyke",
  "repository": {
    "type": "git",
    "url": "https://github.com/kufii/vscode-userscript.git"
  },
  "scripts": {
    "lint": "eslint .",
    "format": "prettier --write \"**/*.{js,jsx,md,json,css,prettierrc,eslintrc}\""
  },
  "engines": {
    "vscode": "^1.38.0"
  },
  "categories": [
    "Programming Languages"
  ],
  "keywords": [
    "userscript",
    "greasemonkey",
    "tampermonkey",
    "violentmonkey",
    "greasyfork"
  ],
  "contributes": {
    "grammars": [
      {
        "scopeName": "text.userjs",
        "path": "./syntaxes/userjs.tmLanguage.json",
        "injectTo": [
          "source.js",
          "source.mjs",
          "source.ts",
          "source.jsx",
          "source.tsx"
        ]
      }
    ]
  },
  "devDependencies": {
    "eslint": "^6.4.0",
    "eslint-config-adpyke-es6": "^1.4.12",
    "eslint-config-prettier": "^6.3.0",
    "prettier": "^1.18.2"
  }
}
